<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_nomor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_nomor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_nomor"/>
    <TextView
        android:id="@+id/tv_tanggal_pelatihan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Pelatihan"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_nomor"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_tanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:layout_marginStart="30dp"
        app:layout_constraintStart_toEndOf="@id/tv_tanggal_pelatihan"
        app:layout_constraintTop_toTopOf="@id/tv_tanggal_pelatihan"/>
    <TextView
        android:id="@+id/tv_jam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jam Nikah"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_tanggal_pelatihan"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_jam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_jam"/>
    <TextView
        android:id="@+id/tv_lokasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lokasi"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_jam"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_lokasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_lokasi"/>
    <TextView
        android:id="@+id/tv_pelatih"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Penghulu"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/value_lokasi"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_pelatih"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_pelatih"/>
    <TextView
        android:id="@+id/tv_nama_pria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Pria"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/value_pelatih"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_nama_pria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_nama_pria"/>
    <TextView
        android:id="@+id/tv_nama_wanita"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Wanita"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/value_nama_pria"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_nama_wanita"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_nama_wanita"/>
    <TextView
        android:id="@+id/tv_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Status"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/value_nama_wanita"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/value_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintStart_toStartOf="@id/value_tanggal"
        app:layout_constraintTop_toTopOf="@id/tv_status"/>
</androidx.constraintlayout.widget.ConstraintLayout>